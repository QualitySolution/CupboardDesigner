  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT nomenclature.article, nomenclature.name as nomenclature, nomenclature.lenght, nomenclature.widht, nomenclature.height, nomenclature.plush, nomenclature.plusl, materials.name as material, facing.name as facing, count, comment FROM order_components
LEFT JOIN nomenclature ON nomenclature.id = nomenclature_id
LEFT JOIN materials ON materials.id = material_id
LEFT JOIN facing ON facing.id = facing_id
 WHERE order_id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>=Parameters!id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
